{
  "$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
  "name": "Google Drive Connector",
  "id": "google-drive-connector",
  "appliesTo": ["bpmn:ServiceTask"],
  "icon": {
    "contents": "data:image/jpeg;base64,"
  },
  "properties": [
    {
      "label": "Ação",
      "type": "Dropdown",
      "choices": [
        { "name": "Listar arquivos", "value": "get_file_list" },
        { "name": "Download arquivo", "value": "download_file" },
        { "name": "Upload arquivo", "value": "upload_file" }
      ],
      "binding": {
        "type": "camunda:inputParameter",
        "name": "googleDriveAction"
      },
      "constraints": {
        "notEmpty":true,
        "pattern":{
            "message":"Selecione um metodo"
        }
      }
    },
    {
      "label": "Conexão",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "connection"
      }
    },
    {
      "label": "Nome da Pasta",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "folderName"
      }
    },
    {
      "label": "Id do Arquivo",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "fileId"
      }
    },
    {
      "label": "Nome do Arquivo",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "fileName"
      }
    },
    {
      "label": "Conteúdo do Arquivo",
      "type": "Text",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "fileContent"
      }
    },
    {
      "label": "Lista de arquivos",
      "type": "String",
      "editable": false,
      "binding": {
        "type": "camunda:outputParameter",
        "source": "response"
      }
    },
    {
      "label": "Conteúdo",
      "type": "String",
      "editable": false,
      "binding": {
        "type": "camunda:outputParameter",
        "source": "response"
      }
    },
    {
      "label": "Id do Arquivo",
      "type": "String",
      "editable": false,
      "binding": {
        "type": "camunda:outputParameter",
        "source": "response"
      }
    }
  ]
}
